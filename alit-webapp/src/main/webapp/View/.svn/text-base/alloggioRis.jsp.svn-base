<%@ page language = "java" contentType="text/html; charset=utf-8" %><%@ include file="snippets/locale_resolver.jsp" %><%@ page import="anna.alit.servicexhtml.AlloggioRisXHTML"%><% String context = request.getContextPath(); %><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" dir="ltr"><head>  <script language="javascript">    function ChargeScheda(url) {      var context = "${pageContext.request.contextPath}/View/";      url = context + url;      open(url, "ChargeScheda");    }  </script>  <jsp:include page="snippets/head.jsp" >      <jsp:param name="title" value="ttl.alloggioRis" />  </jsp:include></head><body><div id="wrapper"><div id="wrapper_inner">  <jsp:include page="snippets/new_header.jsp"/>  <div id="main">	<div class="left" id="content">		<div id="content_inner">		  <div class="post">			<div class="post_title">			 <h2>			   <a href="#">                 <% String[] d = (String[]) session.getAttribute("denominazione"); %>				 <b><%= d.length %> <fmt:message key="msg.risultati_ottenuti"/></b>			   </a>			 </h2>			</div>            <div>              <jsp:include page="alloggioRis_ordinamento.jsp" />		      <form name="One" action="${pageContext.request.contextPath}/AggiornaClickeIP" method="post">	            <input type="hidden"  name="id" id="id" value="" />		              <%		              String[] id = (String[]) session.getAttribute("id");		              String[] denominazione = (String[]) session.getAttribute("denominazione");		              String[] tipoesteso = (String[]) session.getAttribute("tipoesteso");		              String[] regione = (String[]) session.getAttribute("regione");		              String[] comune = (String[]) session.getAttribute("comune");		              String[] sigla = (String[]) session.getAttribute("sigla");		              String[] numcamere = (String[]) session.getAttribute("numcamere");		              String[] numlettitot = (String[]) session.getAttribute("numlettitot");		              String[] prezzomedio = (String[]) session.getAttribute("prezzomedio");		              String[] scontodel = (String[]) session.getAttribute("scontodel");		              String p = (String) request.getParameter("pagina");		              int pagina = 1;		              if(p != null){		                pagina = Integer.parseInt(p);		                session.setAttribute("pagina", pagina);		              }		              int index = ( pagina -1 ) * 15;		              int size = denominazione.length;		              int maxpagine = size / 15;		              if((size % 15) != 0)		                ++maxpagine;		              String folder = request.getContextPath();		              int counter;		              for(counter = 0; counter < 15 && index < size && index >= 0; ++counter)		              {		                out.println(AlloggioRisXHTML.getTableRow(context, folder, denominazione, id, tipoesteso, regione, comune, sigla,		                  numcamere, numlettitot, prezzomedio, scontodel, index));		                index++;		              }		              %>		        <%		          String ipremoto = request.getRemoteAddr();		          String server = request.getServerName();		        %>		        <input type="hidden" id="ipnumerico" name="ipnumerico" value="<%=ipremoto %>" />		        <input type="hidden" id="iplettere" name="iplettere" value="<%=server %>" />		      </form>		      </div>			  <div>			  &nbsp;			  </div>			  <div style="clear:both">             <br/>		      <form class="index" name="risultati" action="${pageContext.request.contextPath}/View/alloggioRis.jsp">		        <p class="index_sx">		          <a class="index" href="<%= context + response.encodeURL("/View/home.jsp")%>">		            <fmt:message key="lnk.EFFETTUA_UNA_NUOVA_RICERCA_"/>		          </a>		        </p>		        <p class="index_dx">		          <jsp:include page="snippets/indice.jsp" >		              <jsp:param name="url" value="/View/alloggioRis.jsp?pagina=" />		              <jsp:param name="pagina" value="<%= pagina %>" />		              <jsp:param name="maxpagine" value="<%= maxpagine %>" />		          </jsp:include>		        </p>		      </form>		      </div>		 </div>		</div>	 </div>     <jsp:include page="snippets/new_right.jsp">       <jsp:param name="title" value="ttl.alloggioRis" />       <jsp:param name="header_text" value="hdr.cerca_alloggio_3" />     </jsp:include>     <jsp:include page="snippets/new_footer.jsp" />  </div></div></div></body></html>