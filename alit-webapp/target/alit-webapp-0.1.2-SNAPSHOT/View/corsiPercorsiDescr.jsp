<%@ page language = "java" contentType="text/html; charset=utf-8" %><%@ include file="snippets/locale_resolver.jsp" %><%@ page import="  anna.alit.servicebb.JspServiceImpl,  anna.alit.beanbb.HashBean,  anna.alit.beanbb.HashBeanVector,  anna.alit.servicebb.GetSegnalazioniDataImpl"%><%    String context = request.getContextPath();	String id = request.getParameter("id");	HashBean hashbean = null;	if(id != null)	{	  hashbean = GetSegnalazioniDataImpl.getInstance().getData(id);	  session.setAttribute("hashbean", hashbean);	}	else	{	  hashbean = (HashBean) session.getAttribute("hashbean");	}	if(hashbean == null)	{	    response.sendError(HttpServletResponse.SC_NOT_FOUND);	}    String eventName = hashbean.get("nome");    String category = (hashbean.get("categoria") != null) ? hashbean.get("categoria") : "";    String place = (hashbean.get("comu") != null) ? hashbean.get("comu") : "";    String province = (hashbean.get("prov") != null) ? hashbean.get("prov") : "";    String county = (hashbean.get("regione") != null) ? hashbean.get("regione") : "";%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml" dir="ltr"><head>  <title>scheda evento: <%=eventName %> <%=category %> <%=place %> <%=province %> <%=county %></title>  <meta http-equiv="content-type" content="text/html" />  <meta name="description" content="scheda di descrizione dell'evento <%=eventName %> <%=category %> <%=place %> <%=province %> <%=county %>" />  <meta name="keywords" content="<%=eventName %>, <%=category %>, <%=place %>, <%=province %> <%=county %>, caratteristiche, camere, posizione, contatti, distanze" />  <script language="JavaScript">    function salva() {      if (document.all) {        document.execCommand("SaveAs");      }else{      alert('Funzione non supportata dal browser che stai usando');      }    }  </script>  <jsp:include page="snippets/head.jsp" >      <jsp:param name="title" value="ttl.corsiPercorsiDescr" />  </jsp:include></head><body><div id="wrapper"><div id="wrapper_inner">  <jsp:include page="snippets/new_header.jsp"/>  <div id="main">	<div class="left" id="content">		<div id="content_inner">		  <div class="post">			<div class="post_title">			 <h2>			   <a href="#">               <fmt:message key="ttl.corsiPercorsiDescr"/>			   </a>			 </h2>			</div>		      <div class="social">			    <%String thisUrl = request.getRequestURL()+"?"+request.getQueryString(); %>			    <g:plusone size="medium"></g:plusone>                <iframe src="http://www.facebook.com/plugins/like.php?href=<%=thisUrl%>&amp;send=false&amp;layout=button_count&amp;width=100&amp;show_faces=true&amp;action=like&amp;colorscheme=light&amp;font=verdana&amp;height=21" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:100px; height:21px; text-align:right" allowTransparency="true"></iframe>	            <a href="http://twitter.com/share" class="twitter-share-button" data-url="<%=thisUrl%>" data-count="horizontal">Tweet</a><script type="text/javascript" src="http://platform.twitter.com/widgets.js"></script>&nbsp;&nbsp;	            <a href="http://www.netlog.com/go/manage/links/view=save&origin=external&url=<%=thisUrl%>"><img src="http://v4.netlogstatic.com/last/s/i/common/buttons/add_link.png" /></a>	          </div>	          <p>	            <b><%= eventName %></b><br/>	            <%= category %><br/>	            <%= place %>	            <%= "(" + province + ")" %><br/>	            <%= "<i>" + county + "</i>" %><br/>	            <br/><br/>	            <fmt:message key="msg.Segnalato_da_"/><br/>	            <% String codice = hashbean.get("idazienda"); %>	            <b>	              <a href="${pageContext.request.contextPath}/View/scheda1.jsp?id=<%=codice%>" >	                <%= hashbean.get("denominazione") %>	              </a>	            </b>	            <br/>	            <%= (hashbean.get("tipoesteso") != null) ? hashbean.get("tipoesteso") : "" %><br/>	            <%= (hashbean.get("comune") != null) ? hashbean.get("comune") : "" %>	            <%= (hashbean.get("sigla") != null) ? "(" + hashbean.get("sigla") + ")" : "" %><br/>	          </p>	          <br/> <br/>	          <p>	           <%= (hashbean.get("descrizione2") != null) ? hashbean.get("descrizione2") : "" %>	          </p>	          <p>	            <button type="button" onclick="window.close()">	              <span class="round_170"><span><fmt:message key="btn.chiudi_questa_scheda"/></span></span>	            </button><br/><br/>	            <button type="button" onclick="salva()">	              <span class="round_170"><span><fmt:message key="btn.salva_questa_scheda"/></span></span>	            </button><br/><br/>	            <button type="button" onclick="window.print()">	              <span class="round_170"><span><fmt:message key="btn.stampa_questa_scheda"/></span></span>	            </button>	          </p>		  </div>		</div>	 </div>     <jsp:include page="snippets/new_right.jsp">       <jsp:param name="title" value="ttl.corsiPercorsiDescr" />       <jsp:param name="header_text" value="hdr.corsi_percorsi_2" />     </jsp:include>     <jsp:include page="snippets/new_footer.jsp" />  </div></div></div></body></html>